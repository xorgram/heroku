{
    "name": "Xor",
    "logo": "https://xorgram.github.io/resources/png/rose-rounded.png",
    "description": "The next-gen Telegram enhancer.",
    "repository": "https://github.com/xorgram/xor",
    "env": {
        "STRING_SESSION": {
            "description": "The user string session.",
            "required": true
        },
        "APP_ID": {
            "description": "The Telegram app ID.",
            "required": true
        },
        "APP_HASH": {
            "description": "The Telegram app hash.",
            "required": true
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql",
            "options": {
                "version": "13"
            }
        }
    ],
    "stack": "container"
}